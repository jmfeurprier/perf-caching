{
  "name": "perf/caching",
  "type": "library",
  "license": "MIT",
  "description": "Allows to store data with caching engines (Memcache, file system, etc).",
  "require": {
    "php": ">=7.4",
    "perf/timing": "^2.0"
  },
  "require-dev": {
    "ext-xdebug": "^2.1.0",
    "pdepend/pdepend": "^2.0.0",
    "phing/phing": "^2.0.0",
    "phpmd/phpmd": "^2.0.0",
    "phpunit/phpunit": "^9.2",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "suggest": {
    "ext-memcached": "Memcached storage"
  },
  "autoload": {
    "psr-4": {
      "perf\\Caching\\": "src/"
    }
  }
}
